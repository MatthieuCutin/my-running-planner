<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="nelmio_api_doc.yml_provider.acseo_yml_provider.class">ACSEO\Bundle\MyRunningPlannerBundle\Service\NelmioApiYmlProvider</parameter>
    </parameters>

    <services>
        <service id="nelmio_api_doc.yml_provider.acseo_yml_provider" class="%nelmio_api_doc.yml_provider.acseo_yml_provider.class%">
            <argument>%kernel.root_dir%</argument>
            <tag name="nelmio_api_doc.extractor.annotations_provider" />
        </service>
    </services>
</container>
